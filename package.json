    "highlight.js": "^11.10.0",
    "marked": "^15.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-rnd": "^10.5.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.282",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "sharp": "^0.34.5",
    "typescript": "~5.6.2",
    "vite": "^6.0.11",
    "vitest": "^2.1.8"
  }
